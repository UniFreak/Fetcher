{
    "name": "unifreak/fetcher",
    "description": "fetch data from configured apis",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "homepage": "https://www.github.com/Unifreak/Fetcher",
    "authors": [
        {
            "name": "FangHao",
            "email": "fanghao90s@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Unifreak\\Fetcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unifreak\\Fetcher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Unifreak\\Fetcher\\FetcherServiceProvider"
            ],
            "aliases": {
                "Fetcher": "Unifreak\\Fetcher\\FetcherFacade"
            }
        }
    }
}
